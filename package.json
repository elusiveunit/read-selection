    "js-w": "npm run js -- --watch",
    "js-prod": "cross-env NODE_ENV=production npm run js"
  },
  "dependencies": {
    "lodash": "~4.17.15",
    "webext-base-css": "~1.1.0",
    "webextension-polyfill": "~0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "~10.1.0",
    "cross-env": "~7.0.2",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.1.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "~3.1.3",
    "node-sass": "~4.14.1",
    "node-sass-tilde-importer": "~1.0.2",
    "prettier": "~2.0.5",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11"
  }
}