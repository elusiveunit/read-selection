{
  "name": "read-selection",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css": "node-sass --output-style expanded --importer=node_modules/node-sass-tilde-importer src/options-page.scss dist/options-page.css",
    "css-w": "npm run css && npm run css -- --watch",
